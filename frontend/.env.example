# Frontend Environment Variables Example
# Copy this file to .env.development or .env.production and update the values

# API Configuration
VITE_API_URL=http://localhost:5000/api

# App Configuration
VITE_APP_TITLE=Devnovate Blog

# Development Server Port (optional)
VITE_PORT=3000

# Backend Server URL (for API calls)
VITE_BACKEND_URL=http://localhost:5000

# Environment Mode
VITE_NODE_ENV=development

# Cloudinary Configuration (if needed for frontend uploads)
VITE_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name

# Admin Email (for contact forms, etc.)
VITE_ADMIN_EMAIL=admin@devnovate.com
